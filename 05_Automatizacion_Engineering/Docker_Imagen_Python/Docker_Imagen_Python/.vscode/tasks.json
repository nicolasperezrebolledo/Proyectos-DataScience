{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build Docker Image",
      "type": "shell",
      "command": "docker build --no-cache -t etl-bebidas -f dockerfile/Dockerfile .",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "Run Docker Container",
      "type": "shell",
      "command": "docker run --rm -p 8888:8888 -v \"${workspaceFolder}:/app\" etl-bebidas",
      "dependsOn": "Build Docker Image"
    }
  ]
}



  
  
  
  